{
  "$schema": "http://json-schema.org/draft-04/schema",
  "id": "dynamic",
  "title": "dynamic",
  "type": "object",
  "properties": {
    "path": {
      "type": "string",
      "format": "path",
      "description": "The path to create the component.",
      "visible": false
    },
    "project": {
      "type": "string",
      "description": "The name of the project.",
      "$default": {
        "$source": "projectName"
      }
    },
    "name": {
      "type": "string",
      "description": "component name",
      "alias": "n",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "组件名称："
    },
    "module": {
      "type": "string",
      "description": "module name",
      "alias": "m",
      "$default": {
        "$source": "argv",
        "index": 1
      },
      "x-prompt": "模块名称(不存在则创建此模块):"
    },
    "tabTitle": {
      "type": "string",
      "description": "tab title",
      "x-prompt": "此模块tab的title:"
    },
    "searchFormName": {
      "type": "string",
      "description": "search form name",
      "x-prompt": "搜索栏动态表单的名字:"
    },
    "tableName": {
      "type": "string",
      "description": "table name",
      "x-prompt": "动态表格的名字:"
    },
    "newFormName": {
      "type": "string",
      "description": "table name",
      "x-prompt": "新增表单的名字:"
    },
    "editFormName": {
      "type": "string",
      "description": "table name",
      "x-prompt": "编辑表单的名字:"
    }
  },
  "required": [
    "name",
    "module",
    "tabTitle",
    "searchFormName",
    "tableName",
    "newFormName",
    "editFormName"
  ]
}